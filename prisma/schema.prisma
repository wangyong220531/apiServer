// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Users {
    id       Int    @id @default(autoincrement())
    username String 
    password String
    nickname String
    email    String
    avator   String
}

model Article {
    id Int @id @default(autoincrement())
    name String @unique
    alias String 
    is_delete Boolean @default(false)
}
